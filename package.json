{
  "author": "Andrej Stieben (https://github.com/stieben)",
  "bugs": {
    "email" : "bugs@peerhaven.net",
    "url" : "https://github.com/peerhaven/peerhaven/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "description": "The main Peerhaven documentation package",
  "devDependencies": {
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "markdownlint-cli": "^0.27.1",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "homepage": "https://peerhaven.net",
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@peerhaven/peerhaven",
  "optionalDependencies": {
    "covgen": "^3.2.0",
    "license-checker": "^25.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peerhaven/peerhaven.git"
  },
  "scripts": {
    "commit": "semistandard && markdownlint '**/*.md' && git add . && git-cz",
    "lint:js": "semistandard --verbose | snazzy",
    "lint:md": "markdownlint '**/*.md'"
  },
  "version": "1.1.1"
}
