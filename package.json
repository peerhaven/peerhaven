{
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "devDependencies": {
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "markdownlint-cli": "^0.27.1",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "name": "@peerhaven/peerhaven",
  "optionalDependencies": {
    "covgen": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peerhaven/peerhaven.git"
  },
  "scripts": {
    "covgen": "covgen",
    "cz": "semistandard && markdownlint '**/*.md' && git add . && git-cz",
    "md-lint": "markdownlint '**/*.md'",
    "js-lint": "semistandard --verbose | snazzy"
  },
  "version": "1.1.1"
}
